<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".end">

    <TextView
            android:text="出荷します"
            android:layout_width="533dp"
            android:layout_height="132dp"
            android:id="@+id/text" android:textSize="36sp"
            android:typeface="normal" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.843" android:layout_marginTop="76dp"/>
    <Button
            android:text="もう一度"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/endbtn" android:visibility="invisible"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="310dp"
            android:layout_height="350dp" app:srcCompat="@drawable/water_15"
            android:id="@+id/imageView" android:layout_marginTop="136dp"
            app:layout_constraintTop_toBottomOf="@+id/text" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.558"/>
</android.support.constraint.ConstraintLayout>